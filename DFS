#include <stdio.h>

#define MAX 100

int visited[MAX];

void dfs(int graph[][MAX], int node, int n) {
    printf("%d ", node);
    visited[node] = 1;

    for (int i = 0; i < n; i++) {
        if (graph[node][i] == 1 && !visited[i]) {
            dfs(graph, i, n);
        }
    }
}

int main() {
    int n = 6;
    int graph[MAX][MAX] = {0};

    // Sample graph (adjacency matrix)
    graph[0][1] = 1;
    graph[0][2] = 1;
    graph[1][3] = 1;
    graph[1][4] = 1;
    graph[2][4] = 1;
    graph[3][5] = 1;
    graph[4][5] = 1;

    for (int i = 0; i < n; i++) visited[i] = 0;

    printf("DFS Traversal: ");
    dfs(graph, 0, n);

    return 0;
}

#include <stdio.h>

int main() {
    int reg[50], n, key, i, found = 0;
    printf("Enter number of students: ");
    scanf("%d", &n);

    printf("Enter registration numbers: ");
    for (i = 0; i < n; i++) scanf("%d", &reg[i]);

    printf("Enter reg no to search: ");
    scanf("%d", &key);

    for (i = 0; i < n; i++) {
        if (reg[i] == key) {
            printf("Reg no %d found at position %d\n", key, i+1);
            found = 1;
            break;
        }
    }
    if (!found)
        printf("Reg no %d not found\n", key);

    return 0;
}
